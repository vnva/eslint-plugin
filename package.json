  "author": "Vyacheslav Ananev <vnva.dev@gmail.com> (https://github.com/vnva)",
  "license": "MIT",
  "homepage": "https://github.com/vnva/eslint-plugin",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.12.1",
    "@types/node": "^22.10.2",
    "@typescript-eslint/utils": "^8.18.2",
    "eslint": "^9.17.0",
    "eslint-vitest-rule-tester": "^0.7.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "unbuild": "^3.0.1",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "eslint": "*"
